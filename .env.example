# LLM Provider API Keys
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Tiptap Cloud AI Toolkit Configuration
# The base URL for the Server AI Toolkit API.
TIPTAP_CLOUD_AI_API_URL=https://api.tiptap.dev/v3/ai

# Your secret key for authenticating with the Server AI Toolkit API. Used to generate JWT tokens.
TIPTAP_CLOUD_AI_SECRET=your-secret-key

# Your App ID for the Server AI Toolkit API.
TIPTAP_CLOUD_AI_APP_ID=your-app-id

# Your Tiptap Cloud App ID for collaboration and document management.
TIPTAP_CLOUD_APP_ID=your-tiptap-cloud-app-id

# Your REST API secret for accessing Tiptap Cloud's document management API.
TIPTAP_CLOUD_DOCUMENT_MANAGEMENT_API_SECRET=your-document-management-api-secret
