{
  "name": "ai-toolkit-demos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "backend:install": "cd backend && uv sync",
    "backend:install:dev": "cd backend && uv sync --group dev",
    "backend:dev": "cd backend && uv run uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "backend:start": "cd backend && uv run uvicorn main:app --host 0.0.0.0 --port 8000",
    "dev:full": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "setup": "npm run backend:install && npm install",
    "tools:extract": "node extract-tools.js",
    "tools:extract:python": "node extract-tools.js --python"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.23",
    "@ai-sdk/react": "^2.0.30",
    "@tiptap-pro/ai-toolkit": "^3.0.0-alpha.4",
    "@tiptap-pro/ai-toolkit-ai-sdk": "^3.0.0-alpha.3",
    "@tiptap/react": "^3.3.1",
    "@tiptap/starter-kit": "^3.3.1",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "ai": "^5.0.30",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
